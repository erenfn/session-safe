{
  "name": "Session Safe",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod node dist/index.js",
    "dev": "nodemon --legacy-watch src --ext ts,js --ignore \"src/**/*.spec.ts,src/**/*.json\" --exec \"ts-node src/index.ts\"",
    "build": "NODE_ENV=prod tsc",
    "dockerUp": "node -e \"const fs=require('fs');const env=fs.readFileSync('.env','utf8').match(/NODE_ENV=(.*)/)[1];require('child_process').execSync('docker-compose --env-file .env.'+env+' up',{stdio:'inherit'})\"",
    "dockerDown": "docker-compose down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dockerode": "^4.0.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^5.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/pg": "^8.11.0",
    "chai": "^4.5.0",
    "chai-http": "^5.1.1",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.0",
    "nyc": "^17.1.0",
    "sequelize-cli": "^6.6.2",
    "sinon": "^19.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.1"
  }
}
